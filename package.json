{
  "name": "@aleksander/yaak-dayjs",
  "private": false,
  "displayName": "Yaak Day.js Plugin",
  "description": "A Yaak plugin that provides Day.js date/time manipulation functions as template functions for API testing and development.",
  "version": "0.2.0",
  "author": "Aleksander Walczuk",
  "license": "UNLICENSED",
  "homepage": "https://github.com/aleksanderwalczuk/yaak-dayjs-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleksanderwalczuk/yaak-dayjs-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/aleksanderwalczuk/yaak-dayjs-plugin/issues"
  },
  "contributors": [
    {
      "name": "Aleksander Walczuk",
      "email": "ohleg@windowslive.com",
      "url": "https://github.com/aleksanderwalczuk"
    }
  ],
  "keywords": [
    "yaak",
    "dayjs",
    "plugin"
  ],
  "engines": {
    "node": ">=22"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yaakcli build",
    "test": "vitest",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "semantic-release",
    "publish": "yaakcli publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.0",
    "@semantic-release/npm": "13.1.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "20.14.9",
    "@yaakapp/cli": "0.3.1",
    "eslint": "^9.38.0",
    "prettier": "3.6.2",
    "semantic-release": "25.0.1",
    "typescript": "5.5.2",
    "typescript-eslint": "^8.46.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@yaakapp/api": "0.5.3",
    "dayjs": "1.11.18"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "pnpm-lock.yaml"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
